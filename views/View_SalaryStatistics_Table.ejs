  <!-- Features -->

  <section class="white-section" id="features">
    <table class="table">
        <thead class="thead-light">
          <tr>
            <th scope="col">#</th>
            <th scope="col">Department Name</th>
            <th scope="col">Count Of Employees</th>
            <!-- <th scope="col">Handle</th> -->
          </tr>
        </thead>
        <tbody>
            <% for(var i=0; i < Salary.length; i++) { %>
          <tr>
            
            <th scope="row"><%=`${i}`%></th>
            <td><%=Salary[i].DepName%></td>
            <td><%=Salary[i].num%></td>
            
          </tr>
          <% } %>
    
        </tbody>
      </table>
      
  </section>
  
 
